<h1>Login#login</h1>
<p>Find me in app/views/login/login.html.erb</p>

<br>

<%= form_with model: @user, url: login_check_path ,html: { class: "ui form" }, method: :get do |form| %>

  <div class="field">
    <%= form.label :studentid%>
    <%= form.text_field :studentid, autofocus: true %>
  </div>

  <div class="field">
    <%= form.label :password%>
    <%= form.text_field :password %>
  </div>
  <br>
  <div class="actions">
    <%= form.submit "LOGIN", name: "submit", class: "ui primary button"%>
  </div>
<% end %>


